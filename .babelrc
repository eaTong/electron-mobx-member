{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-decorators-legacy",
        [
          "import",
          {
            "libraryName": "antd",
            "libraryDirectory": "lib",
            "style": true
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        "react-hot-loader/babel",
        [
          "import",
          {
            "libraryName": "antd",
            "libraryDirectory": "lib",
            "style": true
          }
        ]
      ]
    }
  }
}
