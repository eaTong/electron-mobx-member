    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.14.7",
